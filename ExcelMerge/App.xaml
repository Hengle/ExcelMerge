<Application x:Class="ExcelMerge.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ExcelMerge"
             xmlns:System="clr-namespace:System;assembly=mscorlib"
             Startup="Application_Startup">
    <Application.Resources>
        <System:String x:Key="BranchPath">分支路径:</System:String>
        <System:String x:Key="ReversionId">搜索:</System:String>
        <System:String x:Key="ReversionSelect">版本选择:</System:String>
        <System:String x:Key="DiffLabel">对比</System:String>
        <System:String x:Key="ApplyLabel">保存</System:String>
        <System:String x:Key="SimpleHeaderLabel">处理表头</System:String>
    </Application.Resources>
</Application>
